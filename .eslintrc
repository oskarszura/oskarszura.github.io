{
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "extends": [
    "airbnb",
    "prettier"
  ],
  "globals": {
    "window": true,
    "document": true,
    "fetch": true,
    "WebSocket": true,
    "cy": true,
    "Cypress": true,
    "Headers": true,
    "Request": true,
    "btoa": true,
    "before": true
  },
  "env": {
    "jest": true
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "import/no-extraneous-dependencies": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["."]
      }
    }
  }
}
